    <command:details>
      <command:name>Remove-AWSVariables</command:name>
      <command:verb>Remove</command:verb>
      <command:noun>AWSVariables</command:noun>
      <maml:description>
        <maml:para>Remove selected or all AWS profile and session related environment variables.</maml:para>
